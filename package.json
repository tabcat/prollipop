{
  "name": "prollipop",
  "version": "3.0.1",
  "description": "A modded Prolly-tree implementation in Typescript",
  "type": "module",
  "files": [
    "./dist/src",
    "!**/__mocks__"
  ],
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/src/index.js"
    },
    "./compare": {
      "types": "./dist/src/compare.d.ts",
      "import": "./dist/src/compare.js"
    },
    "./cursor": {
      "types": "./dist/src/cursor.d.ts",
      "import": "./dist/src/cursor.js"
    },
    "./impls": {
      "types": "./dist/src/impls.d.ts",
      "import": "./dist/src/impls.js"
    },
    "./interface": {
      "types": "./dist/src/interface.d.ts",
      "import": "./dist/src/interface.js"
    },
    "./utils": {
      "types": "./dist/src/utils.d.ts",
      "import": "./dist/src/utils.js"
    },
    "./database": {
      "types": "./dist/src/database.d.ts",
      "import": "./dist/src/database.js"
    }
  },
  "scripts": {
    "prepublishOnly": "pnpm install && pnpm build",
    "lint": "prettier src test --check",
    "format": "prettier src test --write",
    "build": "tsc",
    "test": "vitest --config vitest.config.ts --run",
    "test:watch": "vitest --config vitest.config.ts",
    "api-docs": "typedoc src/compare.ts src/cursor.ts src/diff.ts src/index.ts src/mutate.ts src/utils.ts",
    "usage": "vitest test/usage.test.ts",
    "ci": "pnpm build && pnpm lint && pnpm test"
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-organize-imports"
    ]
  },
  "lint-staged": {
    "*.ts": "prettier --ignore-unknown --write"
  },
  "keywords": [
    "distributed",
    "deterministic",
    "tree",
    "prolly-tree",
    "immutable",
    "ipfs",
    "ipld",
    "helia"
  ],
  "author": "tabcat",
  "license": "MIT",
  "dependencies": {
    "@ipld/dag-cbor": "^9.2.5",
    "@noble/hashes": "^2.0.0",
    "@tabcat/sorted-sets": "^4.2.2",
    "multiformats": "^13.4.0",
    "uint8arrays": "^5.1.0"
  },
  "devDependencies": {
    "blockstore-core": "^5.0.4",
    "husky": "^9.1.7",
    "interface-blockstore": "^5.3.2",
    "lint-staged": "^16.1.6",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.2.0",
    "tinybench": "^5.0.1",
    "typedoc": "^0.28.12",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}
