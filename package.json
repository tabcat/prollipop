{
  "name": "prollipop",
  "version": "0.0.0",
  "description": "modded prolly-tree with (number, uint8array) tuples for keys",
  "type": "module",
  "files": [
    "./dist/src"
  ],
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/src/index.js"
    },
    "./compare": {
      "types": "./dist/src/compare.d.ts",
      "import": "./dist/src/compare.js"
    },
    "./cursor": {
      "types": "./dist/src/cursor.d.ts",
      "import": "./dist/src/cursor.js"
    },
    "./diff": {
      "types": "./dist/src/diff.d.ts",
      "import": "./dist/src/diff.js"
    },
    "./interface": {
      "types": "./dist/src/interface.d.ts",
      "import": "./dist/src/interface.js"
    },
    "./utils": {
      "types": "./dist/src/utils.d.ts",
      "import": "./dist/src/utils.js"
    }
  },
  "scripts": {
    "check-format": "prettier src test --check",
    "format": "prettier src test --write",
    "build": "tsc",
    "test": "vitest --config vitest.config.ts"
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-organize-imports"
    ]
  },
  "lint-staged": {
    "*.ts": "prettier --ignore-unknown --write"
  },
  "keywords": [
    "distributed",
    "deterministic",
    "tree",
    "prolly-tree",
    "immutable"
  ],
  "author": "tabcat",
  "license": "MIT",
  "dependencies": {
    "@ipld/dag-cbor": "^9.2.0",
    "@noble/hashes": "^1.3.3",
    "@tabcat/ith-element": "^1.0.1",
    "@tabcat/ordered-sets": "^4.1.2",
    "interface-blockstore": "^5.2.10",
    "multiformats": "^13.1.0",
    "uint8arrays": "^5.0.2"
  },
  "devDependencies": {
    "blockstore-core": "^4.4.1",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.1",
    "prettier-plugin-organize-imports": "^3.2.4",
    "typedoc": "^0.26.7",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
  }
}
